doctype html
html lang="ja"
	- $pageNo = 1;
	head
		== Slim::Template.new('./templates/_meta.slim').render(Object.new, :title => "資産について 5.株式 エンディングノート", :description => "", :keywords => "f, o, o")
	body.drawer.drawer--left
		== Slim::Template.new('./templates/_header.slim').render
		#container
			#property_about_stock
				.page_label
					h2 資産について
				.about_stock
					h2 5.株式
					.input__container
						.input__box
							.title 証券会社
							input type="text"
						//証券会社
						.input__box
							.title 銘柄
							input type="text"
						//銘柄
						.input__box
							.title 証券番号等
							input type="text"
						//証券番号等
						.input__box.input_amount
